@{
    Html.RenderPartial("HeaderPartialView");
 }

<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
<!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->
</head>
<body>
    <header>
        <div class="container">
            <div class="row header-top">
                <span class="logo">
                    <a href="Home/Index">INCITY</a>
                </span>
                <ul class="socialnet-icons">
                    <li><img src="~/Content/img/facebook.png"></li>
                    <li><img src="~/Content/img/icon.png"></li>
                    <li><img src="~/Content/img/twiter.png"></li>
                </ul>
            </div>
            <div class="row mainmenu">
            @{
                Html.RenderPartial("MenuPartialView");
            }
            </div>
            <div class="row lozung">
                <div class="mainLozung">
                    Det <span class="colorGreen"> er fint å ha </span> et reklamebyrå med seg på laget.
                </div>
                <div class="checkPortfolio">
                    Sjekk vår portefølje
                </div>
                <div>
                    <a href="/Portfolio/Index" class="whiteButton">Portefølje</a>
                </div>
            </div>
        </div>   
    </header>
    <section class="body-content">
    @RenderBody()
    @{
        Html.RenderPartial("AboutCompanyPartialView");
    }
    </section>
     <footer class="MainFooter">
         <div class="container">
             <div class="row">
                <div class="col col-float col-md-6">
                    <h2 class="logo2">Logo</h2>
                    <p>Vi har et fint sted å være. Et sted som gir oss arbeidslyst og inspirasjon.<br>
                     Du kan komme ned og besøke oss en dag det passer.<br> Du kommer til å like deg. Eller vi kan komme til deg,<br>
                     kanskje møtes et sted på halvveien hvis det passer bedre.</p>
                </div>
                <div class="col col-float col-md-3">
                    <h4>Kontakt info</h5>
                    <p>Pedersgata, 142<br>4014, Stavanger<br><br>incity@gmail.com</p>
                </div>
                <div class="col col-float col-md-3 footermenu">
                    <h4>Quick links</h4>
                    <ul>
                        <li>Tjenester</li>
                        <li>Arbeid</li>
                        <li>Om oss</li>
                        <li>Kontakt oss</li>
                    </ul>
                </div>
                <div class="col col-float col-md-3 newsbrev">
                    <h4>Nyhetsbrev</h4>
                    <p> Abonner på vårt gratis nyhetsbrev.</p>
                    <form>
                        <input type="email" required="required" placeholder="EMAIL">
                        <a href="#">OK</a>
                    </form>
                </div>
            </div>
         </div>
         <div class="container">
             <div class="row">
                 <div class="col col-float col-md-15">
                    &copy; @DateTime.Now.Year – приложение ASP.NET
                 </div>
            </div>
         </div>
    </footer>
   

    

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/ajax")
    @Scripts.Render("~/bundles/respond")
    @Scripts.Render("~/bundles/animatejs")
    @RenderSection("scripts", required: false)
    @RenderSection("ScriptForPage", required: false)
    @RenderSection("ScriptMain", required: false)
    @{
        Html.RenderPartial("CurentLinkPartialView");
    }
</body>
</html>
