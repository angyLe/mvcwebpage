@{
    Html.RenderPartial("HeaderPartialView");
}
@Styles.Render("~/Content/bootstrapCss")

<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
<!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->
</head>
<body>

    <header class="headerWhite" style="background:none">
        <div class="container">
            <div class="row header-top header-top-white">
                <span class="logo">
                    @Html.ActionLink("INCiTY Admin", "Index", "Home")
                </span>
            </div>

        </div>
        <div class="bgOtherPages">
            @ViewBag.PageTitle
        </div>
    </header>
    <section class="body-content-admin">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-1 col-lg-1">
                </div>
                <div class="col-md-10 col-lg-10">
                    @RenderBody()
                </div>
            </div>
        </div>
    </section>

    @RenderSection("Footer", required: false)
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/respond")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("ScriptForPage", required: false)
    @RenderSection("ScriptMain", required: false)
    @{
        Html.RenderPartial("CurentLinkPartialView");
    }

    @RenderSection("NewsLoadScript", required: false)


</body>
</html>

